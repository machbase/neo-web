{
    "name": "taganalyzer-front",
    "version": "0.0.0",
    "scripts": {
        "lint:style": "stylelint src/**/*.{css,scss,vue}",
        "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
        "serve": "vite preview",
        "build": "vite build",
        "build:cmcgdev": "vite build --mode cmcgdev",
        "dev": "vite --mode cmcgdev",
        "dev:cmcgdev": "vite --mode cmcgdev",
        "test:unit": "jest src",
        "deploy:cmcg": "docker-compose -f docker-compose-cmcg.yml up -d --build"
    },
    "lint-staged": {
        "*.{ts,tsx}": "eslint --fix",
        "*.{css,scss,vue}": "stylelint --fix",
        "*": "prettier -w -u"
    },
    "dependencies": {
        "@mdi/font": "5.9.55",
        "axios": "^0.24.0",
        "core-js": "^3.8.3",
        "highcharts": "^10.3.1",
        "highcharts-vue": "^1.4.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "vue": "^3.2.6",
        "vue-router": "^4.0.11",
        "vuetify": "^3.0.3",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@vuepic/vue-datepicker": "^3.5.2",
        "@originjs/vite-plugin-commonjs": "^1.0.3",
        "@types/jest": "^28.1.4",
        "@types/lodash": "^4.14.188",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "@vitejs/plugin-vue": "^1.6.0",
        "@vue/cli-plugin-router": "^4.5.13",
        "@vue/cli-plugin-typescript": "^4.0.5",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.5",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^7.0.0",
        "@vue/test-utils": "^2.0.0-rc.18",
        "eslint": "^7.32.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^7.17.0",
        "jest": "^26.6.3",
        "prettier": "^2.3.2",
        "sass": "~1.32.0",
        "sass-loader": "^10.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-recommended": "^5.0.0",
        "stylelint-config-standard": "^22.0.0",
        "ts-jest": "^26.5.6",
        "typescript": "^4.3.2",
        "vite": "^2.5.1",
        "vite-plugin-vue-setup-extend": "^0.4.0",
        "vite-plugin-vuetify": "^1.0.0",
        "vue-cli-plugin-vuetify": "~2.4.2",
        "vue-jest": "^5.0.0-alpha.10",
        "vue-tsc": "^0.2.2"
    },
    "engines": {
        "node": "14.x"
    }
}
